{
  "name": "AElfWebsite-Angular",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/AElfProject/AElfWebsite-Angular.git",
  "author": "atom-young <atom.yang@aelf.io>",
  "license": "MIT",
  "scripts": {
    "dev": "npm run clean && cross-env NODE_ENV=development && webpack-dev-server --progress --colors --display-chunks --watch --config ./build/webpack.dev.js",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --progress --colors --display-chunks --config ./build/webpack.prod.js",
    "postbuild": "react-snap",
    "analyze": "npm run clean && cross-env NODE_ENV=production webpack --config ./build/webpack.analyze.js",
    "clean": "rimraf dist/*",
    "commit": "git-cz"
  },
  "reactSnap": {
    "source": "dist",
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "inlineCss": false
  },
  "dependencies": {
    "@babel/runtime": "^7.8.3",
    "core-js": "^3.6.4",
    "i18next": "^19.0.3",
    "i18next-browser-languagedetector": "^4.0.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "query-string": "5",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-i18next": "^11.3.1",
    "react-router-dom": "^5.1.2",
    "react-use": "^13.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-lodash": "^3.3.4",
    "commitizen": "^4.0.3",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-security": "^1.4.0",
    "file-loader": "^5.0.2",
    "git-cz": "^4.1.0",
    "glob": "^7.1.6",
    "html-webpack-plugin": "^4.0.0-beta.11",
    "husky": "^4.0.10",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.5.0",
    "mini-css-extract-plugin": "^0.9.0",
    "minimist": "^1.2.0",
    "moment-locales-webpack-plugin": "^1.1.2",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-less": "^3.1.4",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "react-snap": "^1.23.0",
    "rimraf": "^3.0.0",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^2.3.2",
    "unused-files-webpack-plugin": "^3.4.0",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "node scripts/verify-commit-msg.js",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": "eslint"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "theme": {}
}
